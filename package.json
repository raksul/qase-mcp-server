    "@modelcontextprotocol/sdk": "^1.7.0",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "ts-jest": "^29.3.0",
    "jest": "^29.7.0",
    "axios-mock-adapter": "^2.1.0",
    "@types/jest": "^29.5.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "23.5.0"
  }
}
